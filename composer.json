{
  "name": "opxcore/config-environment",
  "description": "The OpxCore config environment loader.",
  "keywords": [
    "opxcore"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://opxcore.com",
  "support": {
    "issues": "https://github.com/opxcore/config-environment/issues",
    "source": "https://github.com/opxcore/config-environment"
  },
  "require": {
    "php": "^7.4",
    "opxcore/config-environment-interface": "^1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "OpxCore\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpxCore\\Tests\\Config\\Environment\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
